{
    "name": "silent-gear-calculator",
    "description": "A simple web application for creating tools for the Silent Gear Minecraft mod.",
    "version": "0.0.0",
    "license": "SEE LICENSE IN LICENSE.md",
    "private": true,
    "author": {
        "name": "G'lek Tarssza",
        "email": "glek@glektarssza.com",
        "url": "https://glektarssza.com"
    },
    "homepage": "https://glektarssza.github.io/silent-gear-calculator",
    "bugs": {
        "url": "https://github.com/glektarssza/silent-gear-calculator/issues"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/glektarssza/silent-gear-calculator"
    },
    "scripts": {
        "test": "npm run test:ci",
        "test:ci": "karma start --single-run --browsers ChromeHeadless",
        "test:watch": "npm run test:base --single-run --browsers ChromeHeadless --auto-watch",
        "test:debug": "npm run test:base start",
        "test:base": "karma",
        "prepare": "husky",
        "lint": "npm run lint:cache ./src/ts/ ./tests/",
        "lint:cache": "npm run lint:base --cache",
        "lint:base": "eslint --ext .tsx,.ts,.jsx,.js",
        "dev": "webpack serve --config ./webpack.config.dev.ts",
        "build": "npm run build:prod",
        "build:all": "npm run build:dev && npm run build:prod",
        "build:dev": "webpack build --config ./webpack.config.dev.ts",
        "build:prod": "webpack build --config ./webpack.config.prod.ts",
        "clean": "npm run clean:prod",
        "clean:all": "npm run clean:dev && npm run clean:prod",
        "clean:dev": "rimraf ./dist/dev/",
        "clean:prod": "rimraf ./dist/prod/",
        "rebuild": "npm run rebuild:prod",
        "rebuild:all": "npm run rebuild:dev && npm run rebuild:prod",
        "rebuild:dev": "npm run clean:dev && npm run build:dev",
        "rebuild:prod": "npm run clean:prod && npm run build:prod"
    },
    "dependencies": {
        "react": "^18.2.0",
        "react-dom": "^18.2.0"
    },
    "optionalDependencies": {
        "@types/react": "^18.2.55",
        "@types/react-dom": "^18.2.19"
    },
    "devDependencies": {
        "@faker-js/faker": "^8.4.0",
        "@jsdevtools/coverage-istanbul-loader": "^3.0.5",
        "@types/chai": "^4.3.11",
        "@types/karma": "^6.3.8",
        "@types/mocha": "^10.0.6",
        "@types/node": "^20.11.13",
        "@types/sinon": "^17.0.3",
        "@types/sinon-chai": "^3.2.12",
        "@types/webpack": "^5.28.5",
        "@typescript-eslint/eslint-plugin": "^6.20.0",
        "@typescript-eslint/parser": "^6.20.0",
        "chai": "^4.4.1",
        "css-loader": "^6.10.0",
        "cssnano": "^6.0.3",
        "eslint": "^8.56.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-prettier": "^5.1.3",
        "eslint-plugin-tsdoc": "^0.2.17",
        "eslint-webpack-plugin": "^4.0.1",
        "fork-ts-checker-webpack-plugin": "^9.0.2",
        "husky": "^9.0.7",
        "karma": "^6.4.2",
        "karma-chrome-launcher": "^3.2.0",
        "karma-coverage-istanbul-reporter": "^3.0.3",
        "karma-mocha": "^2.0.1",
        "karma-sourcemap-loader": "^0.4.0",
        "karma-spec-reporter": "^0.0.36",
        "karma-webpack": "^5.0.0",
        "mocha": "^10.2.0",
        "postcss": "^8.4.33",
        "postcss-loader": "^8.1.0",
        "pug-plugin": "^4.9.9",
        "puppeteer": "^21.10.0",
        "rimraf": "^5.0.5",
        "sass": "^1.70.0",
        "sass-loader": "^14.1.0",
        "sinon": "^17.0.1",
        "sinon-chai": "^3.7.0",
        "source-map-loader": "^5.0.0",
        "ts-loader": "^9.5.1",
        "ts-node": "^10.9.2",
        "tsconfig-paths-webpack-plugin": "^4.1.0",
        "typescript": "^5.3.3",
        "webpack": "^5.90.0",
        "webpack-cli": "^5.1.4",
        "webpack-dev-server": "^4.15.1",
        "webpack-merge": "^5.10.0"
    }
}
