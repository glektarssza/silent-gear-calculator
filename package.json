{
    "name": "silent-gear-calculator",
    "description": "A simple web application for creating tools for the Silent Gear Minecraft mod.",
    "version": "0.0.0",
    "license": "SEE LICENSE IN LICENSE.md",
    "private": true,
    "author": {
        "name": "G'lek Tarssza",
        "email": "glek@glektarssza.com",
        "url": "https://glektarssza.com"
    },
    "homepage": "https://glektarssza.github.io/silent-gear-calculator",
    "bugs": {
        "url": "https://github.com/glektarssza/silent-gear-calculator/issues"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/glektarssza/silent-gear-calculator"
    },
    "scripts": {
        "test": "npm run test:base",
        "test:ci": "npm run test:coverage",
        "test:watch": "npm run test:base --watch",
        "test:coverage": "npm run test:base --coverage",
        "test:base": "wtr",
        "prepare": "husky",
        "lint": "npm run lint:cache ./src/ts/ ./tests/",
        "lint:cache": "npm run lint:base --cache",
        "lint:base": "eslint --ext .tsx,.ts,.jsx,.js",
        "dev": "webpack serve --config ./webpack.config.dev.ts",
        "build": "npm run build:prod",
        "build:all": "npm run build:dev && npm run build:prod",
        "build:dev": "webpack build --config ./webpack.config.dev.ts",
        "build:prod": "webpack build --config ./webpack.config.prod.ts",
        "clean": "npm run clean:prod",
        "clean:all": "npm run clean:dev && npm run clean:prod",
        "clean:dev": "rimraf ./dist/dev/",
        "clean:prod": "rimraf ./dist/prod/",
        "rebuild": "npm run rebuild:prod",
        "rebuild:all": "npm run rebuild:dev && npm run rebuild:prod",
        "rebuild:dev": "npm run clean:dev && npm run build:dev",
        "rebuild:prod": "npm run clean:prod && npm run build:prod"
    },
    "dependencies": {
        "react": "^18.2.0",
        "react-dom": "^18.2.0"
    },
    "optionalDependencies": {
        "@types/react": "^18.2.55",
        "@types/react-dom": "^18.2.19"
    },
    "devDependencies": {
        "@faker-js/faker": "^8.4.1",
        "@types/chai": "^4.3.14",
        "@types/mocha": "^10.0.6",
        "@types/node": "^20.11.30",
        "@types/sinon": "^17.0.3",
        "@types/webpack": "^5.28.5",
        "@typescript-eslint/eslint-plugin": "^7.2.0",
        "@typescript-eslint/parser": "^7.2.0",
        "@web/dev-server-esbuild": "^1.0.2",
        "@web/test-runner": "^0.18.1",
        "@web/test-runner-puppeteer": "^0.16.0",
        "chai": "^5.1.0",
        "css-loader": "^6.10.0",
        "cssnano": "^6.1.0",
        "eslint": "^8.57.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-prettier": "^5.1.3",
        "eslint-plugin-tsdoc": "^0.2.17",
        "eslint-webpack-plugin": "^4.1.0",
        "fork-ts-checker-webpack-plugin": "^9.0.2",
        "husky": "^9.0.11",
        "postcss": "^8.4.37",
        "postcss-loader": "^8.1.1",
        "pug": "^3.0.2",
        "pug-plugin": "^5.0.3",
        "sass": "^1.72.0",
        "sass-loader": "^14.1.1",
        "sinon": "^17.0.1",
        "source-map-loader": "^5.0.0",
        "ts-loader": "^9.5.1",
        "ts-node": "^10.9.2",
        "typescript": "^5.4.3",
        "webpack": "^5.90.3",
        "webpack-cli": "^5.1.4",
        "webpack-dev-server": "^5.0.4",
        "webpack-merge": "^5.10.0"
    }
}
